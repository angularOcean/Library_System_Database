<br>
<div class="insert">
    <form method="POST" id= "insert {{ title }}" autocomplete="off" align>
        <legend><strong>Add {{ title }} </strong></legend>
          <fieldset class="fields">
             {% for index in range(headings| length()) %}
              {% if headings[index] != "ID" %}
                <label for='{{headings[index]}}'> {{headings[index]}} </label>
                {% if headings[index] == "Email" %}
                  <input type="email" id= '{{headings[index]}}' name= '{{headings[index]}}' required><br><br> 
                {% else %}                  
                  <input type="text" id = '{{headings[index]}}' name= '{{headings[index]}}' required><br><br>
                {% endif %}
              {% endif %} 
            {% endfor %}            
        </fieldset>
          <input class="btn btn-primary" type="submit" id="add {{ title }}" value="Add {{ title }}">
          <input class="btn btn-secondary" type="reset" value="Clear" onClick="">
	</form> 
</div>